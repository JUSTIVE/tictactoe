<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.benkim.tictactoe.Main2Activity">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/act2_player1ID"
            android:layout_alignParentTop="true"
            app:layout_constraintTop_toBottomOf="@+id/imageButton12"
            android:layout_marginTop="8dp"
            app:layout_constraintLeft_toLeftOf="@+id/imageButton12"
            app:layout_constraintRight_toRightOf="@+id/imageButton12"
            android:textStyle="normal|bold" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/status"
            android:layout_alignParentTop="true"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton12"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/act2_player2ID"
            android:layout_alignParentTop="true"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton13"
            android:gravity="right"
            app:layout_constraintLeft_toLeftOf="@+id/imageButton13"
            app:layout_constraintRight_toRightOf="@+id/imageButton13"
            android:textStyle="normal|bold" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area2"
            android:padding="1dp"
            android:layout_marginStart="8dp"
            app:layout_constraintLeft_toRightOf="@+id/area1"
            app:layout_constraintRight_toLeftOf="@+id/area3"
            android:layout_marginEnd="8dp"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:antialias="true"
            app:layout_constraintTop_toBottomOf="@+id/act2_player1ID"
            android:layout_marginTop="24dp" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area3"
            android:padding="1dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_width="96dp"
            android:layout_height="96dp"
            app:layout_constraintTop_toBottomOf="@+id/act2_player1ID"
            android:antialias="true"
            android:layout_marginTop="24dp" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area1"
            android:padding="1dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="96dp"
            android:layout_height="96dp"
            app:layout_constraintTop_toBottomOf="@+id/act2_player1ID"
            android:antialias="true"
            android:layout_marginTop="24dp" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area5"
            android:padding="1dp"
            android:layout_width="96dp"
            android:layout_height="96dp"
            app:layout_constraintTop_toBottomOf="@+id/area2"
            android:layout_marginTop="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintLeft_toRightOf="@+id/area4"
            app:layout_constraintRight_toLeftOf="@+id/area6"
            android:antialias="true"
            android:layout_marginEnd="8dp" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area6"
            android:padding="1dp"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/area3"
            android:antialias="true"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area9"
            android:padding="1dp"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:antialias="true"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/area6" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area7"
            android:padding="1dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:antialias="true"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/area4" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area8"
            android:padding="1dp"
            android:layout_width="96dp"
            android:layout_height="96dp"
            app:layout_constraintRight_toLeftOf="@+id/area9"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            app:layout_constraintLeft_toRightOf="@+id/area7"
            android:antialias="true"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/area5" />

        <ImageButton
            app:srcCompat="@android:color/darker_gray"
            android:id="@+id/area4"
            android:padding="1dp"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:antialias="true"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/area1" />

        <ImageButton
            app:srcCompat="@drawable/circle_icon"
            android:id="@+id/imageButton12"
            android:antialias="true"
            android:background="@android:color/transparent"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="16dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageButton
            app:srcCompat="@drawable/ic_close"
            android:id="@+id/imageButton13"
            android:background="@android:color/transparent"
            android:antialias="true"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_width="96dp"
            android:layout_height="96dp" />

        <android.support.design.widget.FloatingActionButton
            android:layout_width="48dp"
            android:layout_height="48dp"
            android:clickable="true"
            app:fabSize="mini"
            app:srcCompat="@drawable/refresh"
            android:id="@+id/act2_refreshFAB"
            android:layout_marginEnd="16dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp" />

    </android.support.constraint.ConstraintLayout>
</RelativeLayout>
